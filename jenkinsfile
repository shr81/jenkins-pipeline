pipeline {
 agent { docker { image 'python:latest' }}
 stages {
 stage('shr-pipeline-test1') {
     steps {
         echo 'test1 for pipeline by Shr'
         sh "python --version"
           }
                             }
 stage('shr-pipeline-test2') {
     steps {
         echo 'test2 for pipeline by Shr'
           }
     }
 stage('shr-pipeline-test3') {
     steps {
         echo 'test3 for pipeline by Shr'
         sh "ls -lah "
           }
      }
   }
}
